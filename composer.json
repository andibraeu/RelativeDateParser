{
    "name": "enko/relativedateparser",
    "description": "A PHP library for parsing relative date expressions like 'next monday' or 'every 2nd friday'",
    "type": "library",
    "keywords": ["date", "parser", "relative", "time"],
    "homepage": "https://github.com/andibraeu/RelativeDateParser",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/translation": "^7.2",
        "symfony/config": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": { "enko\\": "src/enko/" }
    },
    "authors": [
        {
            "name": "Tim Schumacher",
            "email": "tim@bandenkrieg.hacked.jp"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
